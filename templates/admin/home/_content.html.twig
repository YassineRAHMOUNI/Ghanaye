<div style="overflow-x:auto;">
                    <h3>Members Pending Approval</h3> <br>
                    <table class="table" >
                        <tbody>
                        <tr>
                            <th>Id</th>
                            <th>Name surname</th>
                            <th>Email</th>
                            <th>Password</th>
                            <th>Telephone</th>
                            <th>Adress</th>
                            <th>Type</th>
                            <th>Status</th>
                            <th>City</th>
                            <th>Approve</th>
                            <th>Reject</th>
                        </tr>
                        {% for x in Users %}
                            <tr>
                                <td>{{ x.id }}</td>
                                <td>{{ x.name }}</td>
                                <td>{{ x.email }}</td>
                                <td>{{ x.password }}</td>
                                <td>{{ x.tel }}</td>
                                <td>{{ x.addres }}</td>
                                <td>{{ x.type }}</td>
                                <td>{{ x.status }}</td>
                                <td>{{ x.city }}</td>
                                <td> <a href="{{ path('admin_user_succes',{'id':x.id}) }}" class="btn btn-block btn-success btn-xs"><span class="glyphicon glyphicon-ok"></span> &nbsp; &nbsp; Approve</a>  </td>
                                <td> <a href="{{ path('admin_user_deletesucces',{'id':x.id}) }}" onclick="return confirm('Are you sure to delete?')" class="btn btn-block btn-danger btn-xs"><span class="glyphicon glyphicon-remove"></span>&nbsp;&nbsp; Reject</a>  </td>
                            </tr>
                        {% endfor %}
                        </tbody>
                    </table>
                    </div>
